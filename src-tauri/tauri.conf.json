{
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../resources/public/icons",
      "../resources/public/translations",
      "../resources/public/wordlists"
    ],
    "externalBin": ["../onekeepass-proxy/binaries/onekeepass-proxy"],
    "copyright": "",
    "category": "Productivity",
    "shortDescription": "Password Manager",
    "longDescription": "A secure offline password manager",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.15",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "build": {
    "beforeBuildCommand": "",
    "frontendDist": "../resources/public",
    "beforeDevCommand": "",
    "devUrl": "http://localhost:9500"
  },
  "productName": "OneKeePass",
  "mainBinaryName": "OneKeePass",
  "version": "0.18.0",
  "identifier": "com.onekeepass",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "OneKeePass",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "useHttpsScheme": true
      }
    ]
  }
}