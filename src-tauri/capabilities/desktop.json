{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "global-shortcut:default",
    "core:window:default",
    "core:window:allow-set-always-on-top",
    "core:window:allow-unminimize",
    "core:window:allow-set-focus",
    "core:window:allow-minimize"
  ]
}